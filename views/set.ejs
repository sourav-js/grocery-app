<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

<div class="container">
<% if(error && error.length>0){%>
<div class="alert alert-danger">
<%=error %>
</div>
<% } %>
<% if(success && success.length>0){%>
<div class="alert alert-success">
<%=success %>
</div>
<% } %>
</div>






<div class="modal-dialog" id="modin">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Set-Password</h5>
        
      </div>
      <div class="modal-body">

<form action="/setPassword" method="POST">

<div class="form-group">
<input type="hidden" placeholder="username" name="username" value=<%=user.username%> class="form-control" style="width:275px">
</div>

<div class="form-group">
<p>
<input type="password" placeholder="new password" name="password" class="form-control" style="width:275px">
</p>
</div>

<div class="form-group">
<p>
<input type="password" placeholder="confirm password" name="confirm" class="form-control" style="width:275px">
</p>
</div>



<button class="btn btn-primary btn-block">Change-Password</button>
</div>
</form>


      </div>
      
    
   
    </div>
  </div>




